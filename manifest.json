{
  "manifest_version": 2,
  "name": "BlingTabs",
  "version": "1.0",
  "author": "Bling Labs",
  "description": "Simple Tab Search for Chrome.",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "mac": "Alt+O"
      }
    }
  },
  "permissions": [
    "tabs"
  ]
}
